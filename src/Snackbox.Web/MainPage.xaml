<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:components="clr-namespace:Snackbox.Web.Components"
             x:Class="Snackbox.Web.MainPage">

    <BlazorWebView x:Name="BlazorWebView" HostPage="wwwroot/index.html" UrlLoading="OnUrlLoading">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type components:Routes}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
