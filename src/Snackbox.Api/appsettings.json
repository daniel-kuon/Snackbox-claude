{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "YourVerySecretKeyThatIsAtLeast32CharactersLong!",
    "Issuer": "SnackboxApi",
    "Audience": "SnackboxClient",
    "ExpirationMinutes": "60"
  },
  "BarcodeLookup": {
    "ApiKey": "YOUR_API_KEY_HERE"
  }
}
